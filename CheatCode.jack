class CheatCode
{
    field String key;
    field int now;

    constructor CheatCode new(String _key)
    {
        let key = _key;
        let now = 0;
        return this;
    }
    method void reset()
    {
        let now = 0;
        return;
    }
    method boolean complete(char a)
    {
        if(key[now+2] = a) { let now = now+1; }
        else { let now = 0; }
        if(now = key[0]) { return true; }
        return false;
    }
}
